{
  "name": "unused-code-cleaner",
  "version": "1.0.0",
  "description": "Automatically detect and clean unused imports, files, components, and packages from your JavaScript/TypeScript projects",
  "main": "dist/index.js",
  "bin": {
    "unused-cleaner": "dist/cli.js"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "start": "node dist/cli.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "unused",
    "imports",
    "files",
    "cleanup",
    "dependencies",
    "code-analysis",
    "tree-shaking"
  ],
  "author": "Kasim kkn",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.12",
    "simple-git": "^3.20.0",
    "unimported": "^1.31.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kasimkkn/unused-code-cleaner.git"
  },
  "bugs": {
    "url": "https://github.com/kasimkkn/unused-code-cleaner/issues"
  },
  "homepage": "https://github.com/kasimkkn/unused-code-cleaner#readme"
}